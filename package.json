{
  "name": "hello-nextjs",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next",
    "build": "next build",
    "start": "next start",
    "export": "rm -fr .next  && rm -fr out  && rm -fr ./app/html  && next build && next export  -o ./app/html",
    "commit": "git-cz",
    "version": "conventional-changelog -p angular -i CHANGELOG.md -s -r 0",
    "serve": "rm -fr .next  && rm -fr out  && rm -fr ./app/html  && next build && next export  -o ./app/html && serve ./app/html/"
  },
  "dependencies": {
    "@zeit/next-less": "^1.0.1",
    "Base64": "^1.0.2",
    "antd": "^3.22.2",
    "editor.md": "^1.5.0",
    "less": "^3.10.3",
    "markdown-it": "^9.1.0",
    "md5": "^2.2.1",
    "next": "9.0.5",
    "null-loader": "^3.0.0",
    "qrcode": "^1.4.1",
    "react": "16.9.0",
    "react-dom": "16.9.0",
    "react-json-view": "^1.19.1",
    "react-markdown-editor-lite": "^0.4.3"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "gitHooks": {
    "commit-msg": "node scripts/verify-commit-msg.js"
  },
  "devDependencies": {
    "@zeit/next-css": "^1.0.1",
    "autoprefixer": "^9.6.1",
    "babel-plugin-import": "^1.12.1",
    "commitizen": "^4.0.3",
    "conventional-changelog": "^3.1.10",
    "conventional-changelog-cli": "^2.0.23",
    "cz-conventional-changelog": "^3.0.2",
    "git-cz": "^3.2.1",
    "js-beautify": "^1.10.2",
    "less-loader": "^5.0.0",
    "next-compose-plugins": "^2.2.0",
    "next-images": "^1.1.2",
    "uglifyjs-webpack-plugin": "^2.2.0"
  }
}
